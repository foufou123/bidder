<p id="notice"><%= notice %></p>

<h1>Listing Contractors</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Organization type</th>
      <th>Organization name</th>
      <th>Summary</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contractors.each do |contractor| %>
      <tr>
        <td><%= contractor.user_id %></td>
        <td><%= contractor.organization_type %></td>
        <td><%= contractor.organization_name %></td>
        <td><%= contractor.summary %></td>
        <td><%= link_to 'Show', contractor %></td>
        <td><%= link_to 'Edit', edit_contractor_path(contractor) %></td>
        <td><%= link_to 'Destroy', contractor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contractor', new_contractor_path %>
